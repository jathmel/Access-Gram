<h1>Create a post</h1>
<%= form_for @post do |f| %>
  <%= f.label 'Caption' %>
  <%= f.text_field :title %>
  <%= f.label 'Hashtag' %>
  <%= f.text_field :body %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.submit 'Create Post' %>

<% end %>
<%= link_to 'Back to profile', user_path(session[:user_id]) %>
